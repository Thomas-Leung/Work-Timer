{
  "name": "work-timer",
  "version": "0.1.0",
  "description": "A minimalist time for myself.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Work Timer\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Thomas-Leung/Work-Timer.git"
  },
  "author": "Thomas Leung",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Thomas-Leung/Work-Timer/issues"
  },
  "homepage": "https://github.com/Thomas-Leung/Work-Timer#readme",
  "devDependencies": {
    "electron": "^8.0.0",
    "electron-packager": "^14.2.1",
    "electron-reload": "^1.5.0"
  }
}
